@model System.Collections.Generic.IEnumerable<int>
@{
    ViewBag.Title = "Sorteio";
}

<h2>Sorteio</h2>

<div class="row">
    <div class="col-md-12">
        <label>Números Sorteados:</label>
        <table class="table table-responsive">
            <tbody>
                <tr>
                    @foreach (var i in Model)
                    {
                        <td>@i</td>
                    }
                </tr>

            </tbody>
        </table>
    </div>
</div>
